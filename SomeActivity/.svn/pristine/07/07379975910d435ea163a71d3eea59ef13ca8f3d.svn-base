body {
    max-width: 640px;
    margin: 0 auto;
    font-family: Arial, Free Sans;
    background: #b8104b;
}

a:link { text-decoration: none;}
a:active { text-decoration:none;}
a:hover { text-decoration:none;} 
a:visited { text-decoration: none;}

#title img{
	width: 100%;
}

#blank{
	width: 100%;
	text-align: center;
	margin: auto;
	background:#f6f6f6;
}

#blank img{
	width: 80%;
	cursor: pointer;
	background:#f6f6f6;
}

#content_1{
	background:#f6f6f6;
	width: 100%; 
}

.content_1{
	background:#f6f6f6;
	width: 95%;     
	margin: 0 auto;
	text-align: center;
	padding-top: 10px;
}

.contain{
	width: 98%;
	margin: 0 auto;
	background:#f6f6f6;
}

.ad{
	float: left;
	width: 50%;
	background:#f6f6f6;
}

.ad_up{
	width: 92%;
	margin: 0 auto;
	border: 1px solid #b0b0b0;
	border-radius: 2px;
}

.ad_up img{
	width: 100%;
}

.ad_down{
	padding: 5px;
	width: 92%;
	margin: 0 auto;
	height: 40px;
}

#content_2{
	width:100%;
	background: #b8104b;
}

.content_2{
	background: #b8104b;
	width: 95%;
	text-align: center;
	margin: 0 auto;
}

#content_2_left{
	background: #f3f4ee;
	width: 89%;
	float: left;
	padding-bottom: 15px;
}

.content_2_left{
	margin: 0 auto;
	width: 100%;
}

#content_2_right{
	margin-top: 15px;
	width: 10%;
	float: left;
}

#return_top
{
position:fixed; visibility:visible; right:0; bottom: 0;z-index:100000000;
}

#return_top img{
	width:100%;
}
.contain2{
	width: 99%;
	margin: 0 auto;
}

.up{
	width: 100%;
}

.down{
	width: 100%;
	height: 48px;
	margin:0 auto;
	background: #fff3c7;
	color: #df074d;
}

.left{
	float: left;
	margin-left: 2px;
}

.grey{
	color:#706a53;
}

.big_words{
	font-weight: bold;
}

.black{
	color:#5e5e5e;
}

.old_price{
	text-decoration: line-through;
}

.baoyou{
	margin-top: 8px;
}

.right{
	float: right;
}

.right img{
	width:70%;
}

.add{
	float: left;
	border: 1px solid #b0b0b0;
	border-radius: 2px;
	width: 49%;
	margin: 0.5% 0.2%;
}

.add_up{
	margin: 0 auto;
	width: 96.6%;
	padding-top: 5px;
	background-size: 100%;
}

.jifenbao{
	margin-left: 5px;
	background: #ff2d08;
	width: 65%;
	text-align: center;
	color: #fff;
	line-height: 18px;
}

.add_price{
	background: #000;
	width: 41%;
	text-align: center;
	color: #fff;
	line-height: 18px;
	float: right;
	margin-top: 80px;
	opacity:0.5;
}

.add_down{
	background-color: #f4f0f1;
	margin: 5px;
	content: #bfb5ab;
	text-align: left;
	height:55px;
}

.btn{
	width: 100%;
	cursor: pointer;
}

.btn1{
	background: url(../images/whitee_btn.png) no-repeat;
	z-index: 100000;
	position: relative;
	background-size: 100%;
	color:#8F8F8F;
}

.btn2{
	background: url(../images/pinkkk_btn.png) no-repeat;
	position: relative;
	z-index: 10000;
	background-size: 100%;
	color:#fff;
}

.btn3{
	background: url(../images/orange_btn.png) no-repeat;
	position: relative;
	z-index: 1000;
	background-size: 100%;
	color:#fff;
}

.btn4{
	background: url(../images/redddd_btn.png) no-repeat;
	position: relative;
	z-index: 100;
	background-size: 100%;
	color:#fff;
}

.blank_50px{
	width: 100%;
}

.blank_10px{
	height: 10px;
}

.btn_text{
	margin: 0 auto;
	width: 40%;
	height: 100px;
	font-weight: bold;
}

.btn_up span{
	font-weight: bold;
}


.btn_down{
	border-top:  1px #ccc dotted;
	margin-top: 8px;
	padding-top: 8px;
}