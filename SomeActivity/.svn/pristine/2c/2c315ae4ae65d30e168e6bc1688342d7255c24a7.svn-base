	    var num_item = {0:'img/number/0.png',
			            1:'img/number/1.png',
			            2:'img/number/2.png',
			            3:'img/number/3.png',
			            4:'img/number/4.png',
			            5:'img/number/5.png',
			            6:'img/number/6.png',
			            7:'img/number/7.png',
			            8:'img/number/8.png',
			            9:'img/number/9.png'};
	    var dead_num = {11:12009,12:14367,13:11533,14:11986,21:13268,22:12531,23:10822,24:11621,31:15621,32:17892,33:16734,34:12652,41:23356,42:22619,43:23762,44:25693};
    	var menu_item = {1:'stu',2:'off',3:'mom',4:'wife'}; //四个模块
    	var myscroller;
    	
    	function changenum(target,clickNum){
        	var num1 = parseInt(clickNum / 10000);
        	target.find('.cnum1').attr('src',''+num_item[num1]+'');
			var num2 = parseInt((clickNum % 10000) / 1000);
			target.find('.cnum2').attr('src',''+num_item[num2]+'');
			var num3 = parseInt((clickNum % 1000) / 100);
			target.find('.cnum3').attr('src',''+num_item[num3]+'');
			var num4 = parseInt((clickNum % 100) / 10);
			target.find('.cnum4').attr('src',''+num_item[num4]+'');
			var num5 = parseInt(clickNum % 10);
			target.find('.cnum5').attr('src',''+num_item[num5]+'');
        }
    	
        function initt(){
               $.ui.toggleHeaderMenu(false);
               $.ui.removeFooterMenu(false);
	           for(var k=1; k<5; k++){  //4次请求
       				var target = menu_item[k]; //判断哪个区域
       				for(var i=1; i<5; i++){
       					var midnum = k * 10 + i;
       					var clicknum = dead_num[midnum];
       					//解析第一位到第五位数字分别为多少   依次展示
       					changenum($('.'+target+''+i+'_nine'),clicknum);
       				}
	           }
	         //绑定滚动置顶事件
	     		myscroller = $('#pre').scroller({
	     			verticalScroll:true,
	     		   horizontalScroll:false,
	     		   autoEnable:true,
	     		});
	         //监听滚动条
	         $.bind(myscroller,"scrollend",function(){
	        	 $('#to_top').show();
			});
        }
    	var u = navigator.userAgent;//判断安卓苹果
        if (u.indexOf('iPhone') > -1) {//苹果手机
	        $('#afui').scroller({useJsScroll:true});
	        if (!((window.DocumentTouch && document instanceof DocumentTouch) || 'ontouchstart' in window)){
	            var script = document.createElement("script");
	            script.src = "js/plugins/af.desktopBrowsers.js";
	            var tag = $("head").append(script);
	        }
        }else{//其他手机
        	$.feat.nativeTouchScroll=true;
        	$('#to_top').show();
        }
    </script>
