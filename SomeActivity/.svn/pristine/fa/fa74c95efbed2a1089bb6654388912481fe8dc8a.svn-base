            console.log("infinite triggered");
            if($(this.el).find("#infinite").length==0){/*防止上拉出现多个加载*/
                $(this.el).append("<div id='infinite' style='height:60px;line-height:60px;text-align:center;font-weight:bold'>正在加载请稍后...</div>");
