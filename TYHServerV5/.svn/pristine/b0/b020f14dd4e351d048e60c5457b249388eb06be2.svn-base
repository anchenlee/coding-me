package cn.youhui.bean;

/**
 * 用户集分宝帐户信息
 * @author lijun
 * @since 2014-02-10
 */
public class UserJFBAccount {

	/**
	 * 提现成功的
	 */
	private int txNum = 0;
	
	/**
	 * 获得的
	 */
	private int gainNum = 0;
	
	/**
	 * 提现处理中的
	 */
	private int chkTxNum = 0;

	/**
	 * 返利获得
	 */
	private int flNum = 0;
	
	@Override
	public String toString() {
		return "UserJFBAccount [txNum=" + txNum + ", gainNum=" + gainNum
				+ ", chkTxNum=" + chkTxNum + "]";
	}

	public int getTxNum() {
		return txNum;
	}

	public void setTxNum(int txNum) {
		this.txNum = txNum;
	}

	public int getGainNum() {
		return gainNum;
	}

	public void setGainNum(int gainNum) {
		this.gainNum = gainNum;
	}

	public int getChkTxNum() {
		return chkTxNum;
	}

	public void setChkTxNum(int chkTxNum) {
		this.chkTxNum = chkTxNum;
	}

	public int getFlNum() {
		return flNum;
	}

	public void setFlNum(int flNum) {
		this.flNum = flNum;
	}
}
