img{border:none}
li{list-style:none}
h2{margin: 0;padding: 0}
#box{width:100%;height:504px;position:relative;overflow:hidden;margin:0 auto;min-width: 960px}
#pic_list{position:relative;z-index:1;width: 100%;height: 100%}
#pic_list li{height: 100%;width: 100%;position:absolute;left:0;top:0;opacity:0;fliter:alpha(opacity=0);z-index:1;background-position: top center;background-repeat: no-repeat}
#mark{height:52px;width:100%;position:absolute;left:0;bottom:10px;z-index: 5;text-align: center}
.mark{margin: 0 auto;width: 660px;height: 100%;background:#000;opacity:0.65;filter:alpha(opacity=65);position: relative}
#text_list{position:absolute;bottom:7px;left:20px;position:absolute;z-index:5;overflow:hidden;height: 38px;width: 25%;text-align: center}
#text_list h2{display:none;line-height: 18px}
#text_list .show{display:block}
#text_list a{color: #FFFFFF;font-family: "Microsoft YaHei";font-size: 14px;font-weight: normal;text-decoration:none}
#ico_list{position:absolute;bottom:1px;left:262px;width:303px;overflow:hidden;height:46px;z-index:3}
#ico_list ul{width:1050px;position:absolute;left:0;top:0}
#ico_list li{width:68px;float:left;margin-left: 7px}
#ico_list li a{width:68px;display:block;cursor: pointer}
#ico_list li a img{border: 3px solid #fff; height:36px;width:62px;background:#040303;opacity:0.7;filter:alpha(opacity=70);display: block}
/*#ico_list .active{background:url(../images/a_hover.gif) no-repeat center 0;}*/
#ico_list .active img{opacity:1;filter:alpha(opacity=100);border: 3px solid #fe8407; height:36px;width:62px}
.btn{background:url(../images/btn.gif) no-repeat;height:38px;width:38px;position: absolute;bottom:7px;opacity:0.5;filter:alpha(opacity=50);cursor:default;z-index:3}
.showBtn{ opacity:1;filter:alpha(opacity=100);cursor:pointer;z-index:4}
#btn_prev{left:206px}
#btn_next{right:32px;background-position:right 0}