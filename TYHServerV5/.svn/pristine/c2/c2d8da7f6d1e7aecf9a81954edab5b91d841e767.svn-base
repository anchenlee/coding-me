package cn.youhui.bean;

/**
 * 商品浏览记录
 * @author lijun
 *
 */
public class ItemView {
	
	private String uid;
	
	private String itemId;
	
	private String fromChannel;
	
	private String fromValue;
	
	private String imei;
	
	/**
	 * 用户看到的比例
	 */
	private double seeRate;
	
	public ItemView(String uid, String imei, String itemId, String fromChannel,
			String fromValue, double seeRate) {
		super();
		this.uid = uid;
		this.imei = imei;
		this.itemId = itemId;
		this.fromChannel = fromChannel;
		this.fromValue = fromValue;
		this.seeRate = seeRate;
	}

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public String getItemId() {
		return itemId;
	}

	public void setItemId(String itemId) {
		this.itemId = itemId;
	}

	public String getFromChannel() {
		return fromChannel;
	}

	public void setFromChannel(String fromChannel) {
		this.fromChannel = fromChannel;
	}

	public String getFromValue() {
		return fromValue;
	}

	public void setFromValue(String fromValue) {
		this.fromValue = fromValue;
	}

	public double getSeeRate() {
		return seeRate;
	}

	public void setSeeRate(double seeRate) {
		this.seeRate = seeRate;
	}

	public String getImei() {
		return imei;
	}

	public void setImei(String imei) {
		this.imei = imei;
	}
	
}
