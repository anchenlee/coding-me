@CHARSET "UTF-8";
*{padding: 0; margin: 0;}
a{text-decoration: none; color: #999;}
body{background: #e6e6e6; text-align: left; line-height: 1.5;}
#etouch_logo{padding: 22px 0px 10px 0px; background:#fff; border-bottom: 3px solid #f50; text-align: center;}
#etouch_logo img{border: none;margin: 0 auto;}
#m_title{
	padding:5px 20px 10px 20px;
	color:#999;
	font-size: 18px;
	font-weight: bold;
}
#m_products_list{padding: 0 10px;}
#m_products_list ul{background:#fff; width: 100%; border: 1px solid #ccc; list-style: none;}
#m_products_list ul li a{display:block; padding: 10px; border-bottom:1px solid #ccc;background: url("images/m_download.png") no-repeat center right;}
#m_products_list ul li span{display:block; height: 48px; color: #999; font-size: 14px;}
#m_products_list ul li span b{color: #333; font-size: 18px;}
#m_products_list .ic_product{float: left; padding-right: 20px; border: none; vertical-align:middle;}

.btn_back{float: left;}
#m_products_details{margin:0 10px; padding:15px; background: #fff; border: 1px solid #ccc; text-align: center;}
#m_products_details img{margin: 0px auto;padding: 2px; border:1px solid #ccc;}
#m_products_details p{padding:5px; color: #666; font-size: 14px; text-align: left;}
#m_products_download {text-align: center;padding: 10px 0;}
#m_products_download .btn_android_download{display: block;width:301px; height: 53px; margin:0 auto; background: url("images/m_btn_download.png") no-repeat center top; text-indent: -9999px;}
#m_products_download .btn_iPhone_download{display: block;width:301px; height: 53px; margin:10px auto; background: url("images/m_btn_download.png") no-repeat center bottom; text-indent: -9999px;}


#footer{padding:20px; text-align: center; color: #999; font-size: 12px;}
